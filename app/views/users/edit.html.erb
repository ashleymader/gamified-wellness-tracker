<h3>Prefilled information by Google Login, make changes as you wish.</h3><br>

<%= render partial: "layouts/errors", locals: {object: @user} %>

<%= form_for @user do |f| %>
    <%= f.label :username, 'Username:' %>
    <%= f.text_field :username %><br><br>
    <%= f.label :team, 'Select a Team:' %>
    <%= f.select :team_id, options_from_collection_for_select(Team.all, :id, :team_name), prompt: 'Please select a team' %><br><br>
    
    <%= f.submit 'Sign Up/Log in' %>
<% end %>